{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "target": "es6",
    "lib": [
      "es6"
    ]
  },
  "include": [
    "**/*.ts",
    "*.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}