{
  "name": "clean-starter",
  "version": "1.0.0",
  "description": "Clean Architect Starter Application",
  "main": "App.js",
  "repository": "https://github.com/chinhnguyen/clean-starter",
  "author": "Chinh Nguyen",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "featurecpy": "find . -path ./dist -prune -false -o -name \"*.feature\" -exec cp --parent \"{}\" ./dist \\;",
    "start": "npm run build && nodemon local",
    "clean": "tsc --build --clean",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "npm run build && npm run featurecpy && node ./node_modules/cucumber/bin/cucumber-js -f node_modules/cucumber-pretty --exit dist/features/**/*.feature",
    "e2e": "cucumber-js -f node_modules/cucumber-pretty --exit dist/e2e/**/*.feature",
    "coverage": "nyc npm run test",
    "coverage:all": "nyc --all npm run test"
  },
  "dependencies": {
    "@webauthn/server": "^0.1.3",
    "awilix": "^4.2.1",
    "aws-lambda": "^0.1.2",
    "aws-serverless-express": "^3.3.5",
    "base64url": "^3.0.1",
    "body-parser": "^1.18.3",
    "cbor": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "jsrsasign": "^8.0.12",
    "lodash": "^4.17.11",
    "moment": "^2.29.4",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10",
    "spdy": "^4.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.21",
    "@types/aws-serverless-express": "^3.3.0",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.8",
    "@types/cucumber": "^4.0.5",
    "@types/express": "^4.16.1",
    "@types/glob": "^7.1.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.122",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.7.35",
    "chai": "^4.1.2",
    "cucumber": "^5.1.0",
    "cucumber-pretty": "^1.5.0",
    "moq.ts": "^2.8.5",
    "npm-watch": "^0.6.0",
    "nyc": "^13.3.0",
    "source-map-support": "^0.5.11",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.1.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typed-rest-client": "^1.1.2",
    "typescript": "^3.3.3333"
  }
}
